{
  "manifestVersion": 1,
  "id": "delinea-dsv-task",
  "name": "Delinea DSV Azure DevOps Task",
  "version": "0.0.1",
  "publisher": "Delinea",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Azure DevOps task for accessing secrets from Delinea DSV",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/delinea-icon-devops-secrets-vault.png"
  },
  "files": [
    {
      "path": "dsv"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "home": {
      "uri": "https://delinea.com"
    },
    "learn": {
      "uri": "https://delinea.com"
    },
    "support": {
      "uri": "https://delinea.com/support/"
    },
    "repository": {
      "uri": "https://github.com/DelineaXPM/dsv-azure-pipelines-task"
    },
    "issues": {
      "uri": "https://github.com/DelineaXPM/dsv-azure-pipelines-task/issues"
    }
  },
  "contributions": [
    {
      "id": "dsv-azure-pipelines-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "dsv"
      }
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/DelineaXPM/dsv-azure-pipelines-task"
  },
  "galleryFlags": ["Public"]
}
